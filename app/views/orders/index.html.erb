<h1>Orders</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Store Location</th>
    <th>Timestamp</th>
  </tr>

  <% @orders.each do |order| %>
    <tr>
      <td><%= order.name %></td>
      <td><%= order.store_location %></td>
      <td><%= order.created_at %></td>
      <td><%= link_to 'Show', order_path(order) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New',new_order_path %>
